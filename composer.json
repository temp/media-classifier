{
    "name": "temp/media-classifier",
    "type": "library",
    "description": "media classifier",
    "keywords": ["media", "classify", "mimetype"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Wentz",
            "email": "stephan@wentz.it",
            "homepage": "http://www.wentz.it/"
